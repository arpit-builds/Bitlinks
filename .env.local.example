# Rename this file to .env.local and fill in the actual values

# MongoDB connection URI
MONGODB_URI=mongodb+srv://<username>:<password>@cluster0.mongodb.net/<dbname>?retryWrites=true&w=majority

# Public Host (e.g., for generating links)
NEXT_PUBLIC_HOST=http://localhost:3000

# Optional: Repo text or slug for GitHub repo links (if used in app)
NEXT_PUBLIC_REPO_TEXT=your-github-repo